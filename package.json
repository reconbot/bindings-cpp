{
  "name": "@reconbot/bindings-cpp",
  "version": "9.0.7",
  "main": "lib",
  "keywords": [
    "serialport-binding"
  ],
  "dependencies": {
    "@serialport/binding-abstract": "^9.0.7",
    "@serialport/parser-readline": "^9.0.7",
    "debug": "^4.3.1",
    "nan": "^2.14.2",
    "node-gyp-build": "^4.2.3"
  },
  "devDependencies": {
    "@serialport/binding-mock": "^9.0.7",
    "cc": "^3.0.1",
    "chai": "^4.3.0",
    "chai-subset": "^1.5.0",
    "mocha": "^8.3.0",
    "node-abi": "^2.19.3",
    "prebuildify": "^4.1.2",
    "proxyquire": "^2.1.3",
    "sinon": "^9.2.4"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "install": "node-gyp-build",
    "lint": "cc --verbose",
    "prebuild": "prebuildify --all --strip",
    "rebuild": "node-gyp rebuild"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/serialport/node-serialport.git"
  },
  "gypfile": true,
  "cc": {
    "filter": [
      "legal/copyright",
      "build/include"
    ],
    "files": [
      "src/*.cpp",
      "src/*.h"
    ],
    "linelength": "120"
  },
  "funding": "https://opencollective.com/serialport/donate"
}
